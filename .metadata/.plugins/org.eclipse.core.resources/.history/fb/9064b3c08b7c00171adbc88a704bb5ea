package linked;

public class LinkedListsMain {

	public static void main(String[] args) {
		
		LinkedList list = new LinkedList();
		System.out.println(list.add(12));
		System.out.println(list.add(3));
		System.out.println(list.add(14));
		list.printAll();
		System.out.println(list.addFirst(7));
		list.addFirst(100);
		list.printAll();
		LinkedList.Node x = list.removeFirst();
		x.printData();
		list.printAll();
		
		System.out.println(list.isCyclic());
		
	}

}
